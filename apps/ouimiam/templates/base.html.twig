<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8" />
    <!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}{% endblock %}</title>
    <meta name="title" content="">
    <meta name="description" content="">

    <meta name="author" content="Cédric Merouani">
    <meta name="Copyright" content="Copyright Cédric Merouani 2016. All Rights Reserved.">

    {% block stylesheets %}
        {{ encore_entry_script_tags('app') }}
        <link rel="stylesheet" href="/build/app.css" type="text/css"/>
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    {% block javascripts_header %}
    {% endblock %}
</head>
<body>
{% block header %}
    {% embed '_common/header.html.twig' %}{% endembed %}
{% endblock %}

{% block body %}{% endblock %}

{% block footer %}
    {% embed '_common/footer.html.twig' %}{% endembed %}
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
    <script src="/build/app.js"></script>
    <script src="/build/min/jquery.qtip.min.js"></script>
    <script src="/build/min/html5shiv.js"></script>
    <script src="/build/min/owl.carousel.min.js"></script>
    <script src="/build/min/rw-sidebar.min.js"></script>
    <script src="/build/min/smk-accordion.min.js"></script>
    <script src="/build/min/smk-menu.min.js"></script>
    <script src="/build/min/smk-visual-select.min.js"></script>
    <script src="/build/min/scripts.min.js"></script>
{% endblock %}
</body>
</html>